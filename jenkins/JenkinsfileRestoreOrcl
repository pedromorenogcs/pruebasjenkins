pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch:'main', url: 'https://github.com/pedromorenogcs/pruebasjenkins.git'
            }
        }
         stage('Ansible') {
             steps {
                  ansiblePlaybook playbook: 'oci_sin_asm/pruebas/1_initial_database_packages.yaml'
            }
        }
        
    }
}
